{
    "$schema": "https://raw.githubusercontent.com/FLECS-Technologies/app-sdk/main/manifest.schema.json",
    "_schemaVersion": "2.0.0",
    "app": "##APP##",
    "version": "##VERSION##",
    "revision": "0",
    "image": "flecs.azurecr.io/##APP##",
    "multiInstance": false,
    "editor": "",
    "args": [
        "--launch-arg-1",
        "--launch-arg-2=8000"
    ],
    "capabilities": [],
    "conffiles": [
        "default.conf:/etc/my-app/default.conf"
    ],
    "devices": [
        "/dev/net/tun"
    ],
    "env": [
        "MY_ENV=value"
    ],
    "interactive": false,
    "ports": [
        "12345:12345"
    ],
    "volumes": [
        "my-app-etc:/etc/my-app"
    ]
}